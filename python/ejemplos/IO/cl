<script type="text/javascript">
	$(document).ready(function(){
		$("#showNormas").colorbox({width:"750px", height:"600px", inline:true, href:"#normas"});
		$("#showNormas").mouseover(function(){
			if($(this).hasClass("getNormas")){
				$.ajax({
					url: "http://www.clarin.com/getNormas.json",
					jsonp: "callback",
					cache: 'false',
					dataType: "jsonp",
					success: function( response ) {
						$("#normas").html(response.html);
					}
				});
			}
			$(this).removeClass("getNormas");
		});

		$("#showProteccion").colorbox({width:"750px", height:"600px", inline:true, href:"#proteccion"});
		$("#showProteccion").mouseover(function(){
			if($(this).hasClass("getProteccion")){

				$.ajax({
					url: "http://www.clarin.com/getProteccion.json",
					jsonp: "callback",
					cache: 'false',
					dataType: "jsonp",
					success: function( response ) {
						$("#proteccion").html(response.html);
					}
				});
			}
			$(this).removeClass("getProteccion");
		});

        $("#showTerminosFooter").colorbox({width:"750px", height:"600px", inline:true, href:"#terminosFooter"});
        $("#showTerminosFooter").mouseover(function(){
            if($(this).hasClass("getTerminosFooter")){

                $.ajax({
                    url: "http://www.clarin.com/getTerminos.json",
                    jsonp: "callback",
                    cache: 'false',
                    dataType: "jsonp",
                    success: function( response ) {
                        $("#terminosFooter").html(response.html);
                    }
                });
            }
            $(this).removeClass("getTerminosFooter");
        });

	});
</script>

    

	

	
                                 
