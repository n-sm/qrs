<script type="text/javascript">

function getFechaNac(){
    fecNac = paseE2E.getCookie("Pase-fecNac");
    if(!fecNac) {
        return 0;
    }
    return fecNac.substring(0,4);
}

function getGen(){
    gen = paseE2E.getCookie("Pase-genero");
    if(!gen) {
        return "";  
    }
    return paseE2E.getCookie("Pase-genero");
}

function getTemperatura() {
    if($('[data-temperatura]').length == 0) {
        return "";
    }
    return $('[data-temperatura]').data('temperatura');
}

function getReferrer() {
    if ( document.referrer.length == 0) {
        return "";
    }
    var referrer = document.referrer.split('/')[2];
    return referrer.replace(/\./g,"_");
}

function readCookie(name) {
    return (name = new RegExp('(?:^|;\\s*)' + ('' + name).replace(/[-[\]{}()*+?.,\\^$|#\s]/g, '\\$&') + '=([^;]*)').exec(document.cookie)) && name[1];
}

