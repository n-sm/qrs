    cX.deleteCookie("cxSegmentos");
}catch(e){};


var segments = cX.getUserSegmentIds({ 
        persistedQueryId: '66cac97a468750b6772b428abacdc324d99833a4' 
    });

var smartCx=''; // INIT
if (!((segments == null) || (segments.length == 0)))
{
    for (var i = 0; i < segments.length; i++){ 
        segments[i]='cxdmpid=' + segments[i]; 
    }

    smartCx=segments.join(';');

    if(typeof(sas_target)=="undefined" || sas_target==""){
        try{
            var sas_target = smartCx;
        }catch(e){};
    }else{
        try{ 
            if(sas_target[sas_target.length-1]!=";"){
                sas_target+=";";
            } 
            sas_target = sas_target+ smartCx;
        }catch(e){};
    }
    
}    

var sas_pageid = "32872/(home)";
var sas_site_id = "32872";
var sas_pageVideo_id = "228599";

</script>


<!-- layer tripleImpact -->
<div id="bannerTI" style="position:relative;">
    <div id="sas_13955"></div>
