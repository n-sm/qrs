    onLoggedIn: function(response) {
        $('.pase_notLoggedIn').hide();
        $('.pase_LoggedIn').show();
        if (typeof response != "undefined"){
            $('.icon-ingreso').addClass('icon-profile');
            $('.icon-profile').removeClass('icon-ingreso');
            if (response.user.thumbnailURL != ""){
                $('.icon-profile').append("<img class='avatar' src='"+response.user.thumbnailURL+"'/>");
            } else {
                $('.icon-profile').append("<div class='avatar' />");
                $('.avatar').addClass('default');
            }
        }
    },
    onNotLogged: function() {
        $('.pase_notLoggedIn').show();
        $('.pase_LoggedIn').hide();
        $('.icon-profile').remove('avatar');
        $('.icon-profile').addClass('icon-ingreso');
        $('.icon-ingreso').removeClass('icon-profile');
    }
});

