    sas_formatid=13955; //TripleImpact
    if(typeof SmartAdServerAjax != "undefined"){
        if(typeof window.desAds == "undefined") {
            window.desAds = [];
        }
        
        if(window.desAds.indexOf(sas_formatid) == -1) {
            window.desAds.push(sas_formatid);
            SmartAdServerAjax(sas_pageid,sas_formatid,sas_target);
        }

    }
</script>



