    function renderUDesc(res) {

        $.ajax({
            url: "/claclarinv3/desuscribirAlertas.json?email=" + res.user.email  + "&id=" + res.user.UID,
            method:'GET',
            dataType: "json"
        }).done(function(_response) {
            var daxParameters = {
                'source_modal': desMetrics.sourceModal,
                'name': ''
            };
            var colorboxOptions = {width:"478" , height:"265",scrolling: false, closeButton: true, inline: false, className: 'modalAlerta'};
            if(_response.data == "userNotExist"){
                colorboxCustomOptions = {href:"/newsletter/modal/desuscripcion/otro.html"};
                daxParameters.name = 'newsletter.desuscribir.email-no-coincide';
                desMetrics.sendPageViewCandidate(daxParameters);
            }else{
                if(_response.data == "ok") {
                    daxParameters.name = 'newsletter.desuscribir.email-coincide';
                    desMetrics.sendPageViewCandidate(daxParameters);
                    colorboxCustomOptions = {
                        href: "/newsletter/modal/desuscripcion/ok.html",
                        onComplete: function(){
                            $("#mail_suscripto").html(res.user.email);
                        }};
                }else {
                    colorboxCustomOptions = {href:"/newsletter/modal/desuscripcion/error.html"};
                    daxParameters.name = 'newsletter.suscripcion.error';
                    desMetrics.sendPageViewCandidate(daxParameters);

                }
            }
            jQuery.extend(colorboxOptions, colorboxCustomOptions);
            $.colorbox(colorboxOptions);

        })
        .fail(function() {
                    daxParameters.name = 'newsletter.suscripcion.error';
                    desMetrics.sendPageViewCandidate(daxParameters);
                    //$.colorbox({width:"478" , height:"265",scrolling: false, closeButton: true, inline: false, href:/newsletter/modal/desuscripcion/error.html ,className: 'modalAlerta'});
        });
    }


    $(document).ready(function(){

        var emailDes;
        function validarUserPase(res){
            if(res.user.email == mail[1]){
                renderUDesc(res);
            }else{
                // no esta logueado o usuario logueado no coincide con quien quiere desuscribir
                $.cookie("desuscripto-newsletter-email", mail[1], options);
                $.cookie("desuscripto-newsletter", 1, options);
                paseE2E.openModal();
            }
        }

        function validarLogueado(res){
            if(res.user.email == emailDes){
                renderUDesc(res);
            } else {
                var daxParameters = {
                    'source_modal': desMetrics.sourceModal,
                    'name': ''
                };
                daxParameters.name = 'newsletter.desuscribir.email-no-coincide';
                desMetrics.sendPageViewCandidate(daxParameters);
                $.colorbox({width:"478" , height:"265",scrolling: false, closeButton: true, inline: false, href: "/newsletter/modal/desuscripcion/otro.html", className: 'modalAlerta'});
            }
        }
        
        var date = new Date();
        date.setTime(date.getTime() + (10 * 60 * 1000));
        var options = {
            domain: ".clarin.com",
            path: '/',
            expires: date
        }

        var cookie_sub = $.cookie("desuscripto-newsletter");
        //se registro o se logueo para suscribirse
        if( (cookie_sub != null) && (cookie_sub == 1) && (getIdPase() != "")){
            emailDes = $.cookie("desuscripto-newsletter-email");
            $.cookie("desuscripto-newsletter", null, options);
            $.cookie("desuscripto-newsletter-email", null, options);
            gigya.socialize.getUserInfo({ callback: validarLogueado });
        }

        var params = window.location.hash.substring(1);
        if(params != "" && (params.indexOf('panel-login&mail=') != -1)){
            params = params.split("?")[0];
            params = params.split("&");
            var mail = params[1].split("=");
            if(mail[1] != ""){
                // si se quiso agregar a suscribir pero nunca de logueo o registro
                if( getIdPase() != "") {
                    emailDes = $.cookie("desuscripto-newsletter-email");
                    $.cookie("desuscripto-newsletter", null, options);
                    $.cookie("desuscripto-newsletter-email", null, options);
                    gigya.socialize.getUserInfo({ callback: validarUserPase });
                } else {
                    // no esta logueado
                    $.cookie("desuscripto-newsletter", 1, options);
                    $.cookie("desuscripto-newsletter-email", mail[1], options);
                    paseE2E.openModal();
                }
            }
        }
        
    });
    $(document).bind('cbox_closed', function(e){
        if(paseE2E.getCookie("desuscripto-newsletter") != null) {
            var date = new Date();
            paseE2E.setCookie("desuscripto-newsletter", null, date.getTime() + ( 60 * 1000));
            paseE2E.setCookie("desuscripto-newsletter-email", null, date.getTime() + ( 60 * 1000));
        }
    });

