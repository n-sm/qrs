<script type="text/javascript">
	$(document).ready(function(){
		$('footer').hide();
	});

    var el = $(".on-demand").first();

    var updateOnDemand = function(e) {
        e.html('<div class="demand-loading"></div>');
       
        $(window).unbind("scroll.OnDemand");

        stickyHeader();

        $.get(e.data("src"), function(_response) {
            response = BBT.fixJson(_response);
            if (response.error == 0) {
                e.html(response.data);

                e.find(".sticky-spacer > [data-stick]").stickAll(); 

                if ( e.index() != $(".on-demand").last().index() ) {
                    el = e.next(".on-demand");
                    updateBody();
                }else{
                    $('footer').show();
                }
            }
        });
    }

    var isiPad = navigator.userAgent.match(/iPad/i) != null;

    function checkScroll(e) {
        var modificacionLimiteScroll = 0;

        if (isiPad && ($(window).height()>=$(window).width())) {
            modificacionLimiteScroll = 25;
        };

        if ( ( $(window).scrollTop() + $(window).height() ) >= ((e.offset().top)) - modificacionLimiteScroll ) {
            updateOnDemand(e);
        }
    }

    function updateBody () {
        if ($(window).height() >= $("body").height() ) {
            updateOnDemand(el);
        } else {
            $('footer').hide();
            $(window).bind("scroll.OnDemand", function(){
                checkScroll(el);
            })
        }
    }

	if ( typeof el.html() != "undefined" ){
	    updateBody();
	}
</script>

