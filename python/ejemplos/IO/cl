        'year': "",
        'month': "",
        'day': "",
        'hour': "",
        'weekDay': ""
    },
    'loadPaseIdInDefaults': function() {
        if (this.idPase !== '') {
            this.defaults.pase_id = this.idPase;
        }
    },
    'sendRequestToXML': function() {
        $.ajax({
            type: 'GET',
            url: '/pv.xml',
            dataType: 'xml'
        });
    },
    'sendPageViewCandidate': function(params) {
        window.clearTimeout(timeout);
        
        var oneSecond = 1000;
        var scope = this;
        
        timeout = setTimeout(function() {
            scope.loadPaseIdInDefaults();
            params = $.extend(scope.defaults, params);
            var url = scope.baseUrl + '?' + $.param(params) + desMetrics.getParameterLWtoDax();
            scope.sendRequestToXML();
            udm_(url);
        }, oneSecond);
    },
    'getParameterLWtoDax' : function(){

        var params = '';
        var lwm_count_sum = 0;
        var lwm_blocked = "no";

        if($.cookie('lw-tracking')){

            var oData = JSON.parse($.cookie('lw-tracking'));
            lwm_count_sum = oData.urls.length;
            lwm_blocked = (oData.isBlocked) ? "si" : "no";
        }

        params += "&lwm_count=no";
        params += "&lwm_count_sum=" + lwm_count_sum;
        params += "&lwm_block=no";
        params += "&lwm_blocked=" +lwm_blocked;

        return params;

    }
};

