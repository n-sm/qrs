var paseModuleInit = function() {
     $(".pase-module").hover(function () {
            var userPanel = $(this);
            if (userPanel.find('.pase_LoggedIn').is(':visible') && !userPanel.hasClass('opening')){
                userPanel.addClass('active');
                userPanel.addClass('opening');
                userPanel.find(".opciones").slideDown(150, function() {
                    userPanel.removeClass('opening');
                });
            }   
        },function(){
            var userPanel = $(this);
            if (userPanel.hasClass('active')){
                userPanel.removeClass('active');
                userPanel.find(".opciones").slideUp(50);
            }   
        }
    ); 
}

