    var sliderTime574= 5000;
    
                notasSlider574[0] = ['235635','19/07/2016','El oro vuelve a brillar: sube 30% en el año y va por más, impulsado por el efecto Brexit, tasas y dólar',$j.trim('Los analistas internacionales lo recomiendan cada vez más al observar el clima de mayor incertidumbre, como el Brexit y las tasas planchadas'),'http://www.iprofesional.com/notas/235635-El-oro-vuelve-a-brillar-sube-30-en-el-ao-y-va-por-ms-impulsado-por-el-efecto-Brexit-tasas-y-dlar','El oro vuelve a brillar: sube 30% en el año y va por más, impulsado por el efecto Brexit, tasas y dólar',$j('<img src="http://719ec77057a57996613a-9a5081423686babcde0c8e2e3662dda4.r0.cf1.rackcdn.com/adjuntos/jpg/2016/07/300x219_433224.jpg" width="266" height="194" alt="El oro vuelve a brillar: sube 30% en el año y va por más, impulsado por el efecto Brexit, tasas y dólar" />')];
                    notasSlider574[1] = ['236448','03/08/2016','Advierten que si gana Trump, el oro podría subir hasta los u$s1.850 la onza',$j.trim('El dorado metal, que es utilizado como refugio de valor en tiempos díficiles podría aumentar en más de 40% ante la eventual presidencia del candidato'),'http://www.iprofesional.com/notas/236448-Advierten-que-si-gana-Trump-el-oro-podra-subir-hasta-los-us1850-la-onza','Advierten que si gana Trump, el oro podría subir hasta los u$s1.850 la onza',$j('<img src="http://719ec77057a57996613a-9a5081423686babcde0c8e2e3662dda4.r0.cf1.rackcdn.com/adjuntos/jpg/2016/07/300x219_433352.jpg" width="266" height="194" alt="Advierten que si gana Trump, el oro podría subir hasta los u$s1.850 la onza" />')];
                
    var showNoticia574 = function(idx){
        var id = notasSlider574[idx][0];
        notaActual574 = idx;
        
        var imagen = notasSlider574[idx][6];
        var fecha = notasSlider574[idx][1];
        var titulo = $j('<a href="'+notasSlider574[idx][4]+'" style="font-size: 23px" title="'+notasSlider574[idx][5]+'">'+notasSlider574[idx][2]+'</a>');
        var bajada = $j('<a href="'+notasSlider574[idx][4]+'" title="'+notasSlider574[idx][5]+'">'+notasSlider574[idx][3]+'</a>');
        
        $j('#slider574 .imagen').fadeOut('fast', function(){ $j(this).html(imagen).fadeIn('fast'); });
        $j('#slider574 h2 .titulo').html(titulo);
        $j('#slider574 h3 .bajada').html(bajada);
        $j('#slider574 .fecha').html(fecha);
    }
    
    var rotarAutomatico574 = function(){
        $j($j('#slider574 .puntos ul li')[notaActual574]).click();
        notaActual574++;
        if(notaActual574 >= maxNoticias574) notaActual574 = 0;
    }
    
    
    $j(function(){
        $j('#slider574 .puntos ul li').click(function(){ 
            $j('#slider574 .puntos ul li').removeClass('on');
            $j(this).addClass('on');
            showNoticia574($j(this).index());
        });
        
        sliderInterval574 = setInterval('rotarAutomatico574()',sliderTime574);
        
        $j('#slider574').mouseenter(function(){
            clearInterval(sliderInterval574);
        }).mouseleave(function(){
            sliderInterval574 = setInterval('rotarAutomatico574()',sliderTime574);
        });
    });
</script>                            </div>
