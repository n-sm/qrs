    var showNoticia273 = function(idx){
        var id = notasSlider273[idx][0];
        notaActual273 = idx;
        
        var imagen = notasSlider273[idx][6];
        var fecha = notasSlider273[idx][1];
        var titulo = $j('<a href="'+notasSlider273[idx][4]+'" style="font-size: 23px" title="'+notasSlider273[idx][5]+'">'+notasSlider273[idx][2]+'</a>');
        var bajada = $j('<a href="'+notasSlider273[idx][4]+'" title="'+notasSlider273[idx][5]+'">'+notasSlider273[idx][3]+'</a>');
        
        $j('#slider273 .imagen').fadeOut('fast', function(){ $j(this).html(imagen).fadeIn('fast'); });
        $j('#slider273 h2 .titulo').html(titulo);
        $j('#slider273 h3 .bajada').html(bajada);
        $j('#slider273 .fecha').html(fecha);
    }
    
    var rotarAutomatico273 = function(){
        $j($j('#slider273 .puntos ul li')[notaActual273]).click();
        notaActual273++;
        if(notaActual273 >= maxNoticias273) notaActual273 = 0;
    }
    
    
    $j(function(){
        $j('#slider273 .puntos ul li').click(function(){ 
            $j('#slider273 .puntos ul li').removeClass('on');
            $j(this).addClass('on');
            showNoticia273($j(this).index());
        });
        
        sliderInterval273 = setInterval('rotarAutomatico273()',sliderTime273);
        
        $j('#slider273').mouseenter(function(){
            clearInterval(sliderInterval273);
        }).mouseleave(function(){
            sliderInterval273 = setInterval('rotarAutomatico273()',sliderTime273);
        });
    });
