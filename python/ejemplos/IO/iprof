                notasSlider476[0] = ['236392','02/08/2016','El ministro de Agroindustria confirmó que el Gobierno no va a intervenir por el faltante de manteca',$j.trim('“Con el invierno las amas de casa consumen más manteca en la cocina”, señaló el subsecretario de Lechería. La próxima semana se normalizaría la producción'),'http://www.iprofesional.com/notas/236392-El-ministro-de-Agroindustria-confirm-que-el-Gobierno-no-va-a-intervenir-por-el-faltante-de-manteca','El ministro de Agroindustria confirmó que el Gobierno no va a intervenir por el faltante de manteca',$j('<img src="http://719ec77057a57996613a-9a5081423686babcde0c8e2e3662dda4.r0.cf1.rackcdn.com/adjuntos/jpg/2016/08/300x219_433606.jpg" width="266" height="194" alt="El ministro de Agroindustria confirmó que el Gobierno no va a intervenir por el faltante de manteca" />')];
                    notasSlider476[1] = ['236375','02/08/2016',' Por qué los empresarios de la industria láctea prefieren producir queso antes que manteca',$j.trim('El vicepresidente de CRA, Jorge Chemes, analizó la cadena de producción luego de que el ministro de Agroindustria declarara que el Gobierno no intervendrá'),'http://www.iprofesional.com/notas/236375--Por-qu-los-empresarios-de-la-industria-lctea-prefieren-producir-queso-antes-que-manteca',' Por qué los empresarios de la industria láctea prefieren producir queso antes que manteca',$j('<img src="http://719ec77057a57996613a-9a5081423686babcde0c8e2e3662dda4.r0.cf1.rackcdn.com/adjuntos/jpg/2014/05/300x219_400010.jpg" width="266" height="194" alt=" Por qué los empresarios de la industria láctea prefieren producir queso antes que manteca" />')];
                
    var showNoticia476 = function(idx){
        var id = notasSlider476[idx][0];
        notaActual476 = idx;
        
        var imagen = notasSlider476[idx][6];
        var fecha = notasSlider476[idx][1];
        var titulo = $j('<a href="'+notasSlider476[idx][4]+'" style="font-size: 23px" title="'+notasSlider476[idx][5]+'">'+notasSlider476[idx][2]+'</a>');
        var bajada = $j('<a href="'+notasSlider476[idx][4]+'" title="'+notasSlider476[idx][5]+'">'+notasSlider476[idx][3]+'</a>');
        
        $j('#slider476 .imagen').fadeOut('fast', function(){ $j(this).html(imagen).fadeIn('fast'); });
        $j('#slider476 h2 .titulo').html(titulo);
        $j('#slider476 h3 .bajada').html(bajada);
        $j('#slider476 .fecha').html(fecha);
    }
    
    var rotarAutomatico476 = function(){
        $j($j('#slider476 .puntos ul li')[notaActual476]).click();
        notaActual476++;
        if(notaActual476 >= maxNoticias476) notaActual476 = 0;
    }
    
    
    $j(function(){
        $j('#slider476 .puntos ul li').click(function(){ 
            $j('#slider476 .puntos ul li').removeClass('on');
            $j(this).addClass('on');
            showNoticia476($j(this).index());
        });
        
        sliderInterval476 = setInterval('rotarAutomatico476()',sliderTime476);
        
        $j('#slider476').mouseenter(function(){
            clearInterval(sliderInterval476);
        }).mouseleave(function(){
            sliderInterval476 = setInterval('rotarAutomatico476()',sliderTime476);
        });
    });
