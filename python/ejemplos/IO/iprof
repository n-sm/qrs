        <script type="text/javascript">
            var timer;

            function setRefresh(milisegundos) {
                timer = setInterval("doRefresh()", milisegundos);
            }
            
            /***
             * Código para refresh de la página
             */
            function doRefresh() {
                if ( window.location.href.indexOf( 'page_y' ) != -1 ) {
                    //gets the number from end of url
                    var match = window.location.href.split('?')[1].match( /\d+$/ );
                    var page_y_old = match[0];

                    var page_y_new = $j( document.body ).scrollTop();
                    var href = window.location.href;
                    var new_href = href.replace(page_y_old,page_y_new);

                    window.location.href = new_href;
                }else{
                    var page_y = $j( document.body ).scrollTop();
                    window.location.href = window.location.href.split('?')[0] + '?page_y=' + page_y;
                }
            }
            function stopRefresh() {
                clearTimeout(timer);
            }
        </script>
        
