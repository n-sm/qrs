        <script type="text/javascript">
            function addEvent(element, event, fn) {
                if (element.addEventListener) {
                    element.addEventListener(event, fn, false);
                } else if (element.attachEvent) {
                    element.attachEvent('on' + event, fn);
                }
            }

            function loadScript(src, callback) {
                var s,
                r,
                t;
                r = false;
                s = document.createElement('script');
                s.type = 'text/javascript';
                s.src = src;
                s.onload = s.onreadystatechange = function() {
                    if ( !r && (!this.readyState || this.readyState == 'complete') ){
                        r = true;
                        if (callback !== undefined) {
                            callback();
                        }
                    }
                };
                t = document.getElementsByTagName('script')[0];
                t.parentNode.insertBefore(s, t);
            }
            
            addEvent(window, 'load', function(){ loadScript(
                '/userApi/js/iProfesional.js?v=3',
                function () {
                    iProf.loginStatus(function(){
                        if (iProf.status === 'unlogged' || iProf.status === 'no-status') {
                            iProf.loadButtons();
                        } else if (iProf.status === 'logged') {
                            iProf.loadUserAccount();
                        }
                    });
                });
                
                if (windowWidth >= 1300) {
                    $j('.lateralfijo, .redessociales').css({'position': 'absolute', 'top': $j('.wrappernota').offset().top + 'px'});
                }

                if (windowWidth < 1300) {
                    $j('.lateralfijo').css({'position': 'absolute', 'top': $j('.wrappernota').offset().top + 'px'});
                    $j('.redessociales').css({'position': 'absolute', 'top': $j('.contenidonota').offset().top + 'px'});
                }
            });
        </script>
        
