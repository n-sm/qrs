if(!addEvent) {
    function addEvent(element, event, fn) {
        if (element.addEventListener) {
            element.addEventListener(event, fn, false);
        } else if (element.attachEvent) {
            element.attachEvent('on' + event, fn);
        }
    }
}

addEvent(window, 'load', function(){ 
    $j('.publicar-objeto').die('click').live('click', function(){
        var idFila = $j(this).attr('fila');
        var espacio = $j(this).attr('espacio');
        var idInstancia = $j(this).attr('instancia');
        var estado = $j(this).attr('estate');
        
        var accion = (estado == 1) ? 'despublicar' : 'publicar';

        if (estado == 1) {
            if (confirm('Esta acción despublicará el objeto. ¿Desea continuar?')) {
                $j.ajax({
                    async: false,
                    type: 'post',
                    url:    '/admin/filas/functions_noticias.php',
                    data: 'sAction=publicarObjeto&idFila=' + idFila + '&espacio=' + espacio + '&idInstancia=' + idInstancia + '&estado=' + estado,
                    success: function(response){
                        if(response == -1 || response == -2){
                            alerta('Ocurrio un error al ' + accion + ' el objeto! Puede ser que el objeto no tenga contenido asociado, o que la nota asociada no este publicada. Verifiquelo y vuelva a intentarlo. Gracias.');
                        }else{
                            refreshBloque(idFila);
                        }
                    }
                });
            }
        } else {
            $j.ajax({
                async: false,
                type: 'post',
                url:    '/admin/filas/functions_noticias.php',
                data: 'sAction=publicarObjeto&idFila=' + idFila + '&espacio=' + espacio + '&idInstancia=' + idInstancia + '&estado=' + estado,
                success: function(response){
                    if(response == -1 || response == -2){
                        alerta('Ocurrio un error al ' + accion + ' el objeto! Puede ser que el objeto no tenga contenido asociado, o que la nota asociada no este publicada. Verifiquelo y vuelva a intentarlo. Gracias.');
                    }else{
                        refreshBloque(idFila);
                    }
                }
            });
        }
    });

    $j('.publicar-fila').die('click').live('click', function(){
        var idFila = $j(this).attr('fila');
        var estado = $j(this).attr('estate');
        
        var accion = (estado == 1) ? 'despublicar' : 'publicar';
        
        if (estado == 1) {
            if (confirm('Esta acción despublicará la fila. ¿Desea continuar?')) {
               $j.ajax({
                    async: false,
                    type: 'post',
                    url:    '/action/filas/ajax.bloque.php',
                    data: 'action=publicarBloque&idFila=' + idFila + '&estado=' + estado + '&accion=' + accion,
                    success: function(response){
                        if(response == -1 || response == -2){
                            alerta('Ocurrio un error al ' + accion + ' el objeto! Puede ser que el objeto no tenga contenido asociado, o que la nota asociada no este publicada. Verifiquelo y vuelva a intentarlo. Gracias.');
                        }else{
                            refreshBloque(idFila);
                        }
                    }
                }); 
            }
        } else {
            $j.ajax({
                async: false,
                type: 'post',
                url:    '/action/filas/ajax.bloque.php',
                data: 'action=publicarBloque&idFila=' + idFila + '&estado=' + estado + '&accion=' + accion,
                success: function(response){
                    if(response == -1 || response == -2){
                        alerta('Ocurrio un error al ' + accion + ' el objeto! Puede ser que el objeto no tenga contenido asociado, o que la nota asociada no este publicada. Verifiquelo y vuelva a intentarlo. Gracias.');
                    }else{
                        refreshBloque(idFila);
                    }
                }
            });
        }
    });
});

doRefresh = function() {
                var noRefresh = ['Intro_restricciones', 'HOME_Objeto_Autos', 'HOME_Objeto_Codigo_Civil', 'HOME_Objeto_Vinos'];
                console.log('Refresh banners v4...');
                
                Object.keys(eplDoc.epl.spaces).each(function(space){
                    if(noRefresh.indexOf(space) < 0){ 
                        eval('var obj= eplDoc.epl.spaces.'+space+';');
                        if(obj.rendered()){
                            $j('#eplAdDiv'+space).css('min-height', $j('#eplAdDiv'+space).height());
                            eplDoc.epl.reloadSpace(space);
                        }
                    }
                });
}



