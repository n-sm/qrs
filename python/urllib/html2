<script type="text/javascript">

(function(a){a.fn.validateField=function(b){a(this).on({keypress:function(a){var c=a.which,d=a.keyCode,e=String.fromCharCode(c).toLowerCase(),f=b;(-1!=f.indexOf(e)||9==d||37!=c&&37==d||39==d&&39!=c||8==d||46==d&&46!=c)&&161!=c||a.preventDefault()}})}})(jQuery);

$(function(){
	$('#nombre').validateField(' abcdefghijklmnñopqrstuvwxyzáéiou');
	$('#apellido').validateField(' abcdefghijklmnñopqrstuvwxyzáéiou');
	$('#tel').validateField(' 0123456789');
});

$(document).ready(function() {
	$("#mandar").click(function() {
		
		var errorCampos = false;
		var errorMail = false;
		
		_nombre = $('#nombre').val();
		_apellido = $('#apellido').val();
		_email = $('#mail').val();
		_telefono = $('#tel').val();
		_comentario = document.getElementById('comentario').value; 
		_dirigido = $('#to:checked').val();
		
		if( _nombre.length < 1 || _apellido.length < 1 || _email.length < 1 || _telefono.length < 1 || _comentario.length < 1 ) 
			errorCampos = true;
		
		if( !isValidEmailAddress( _email ) ) 
			errorMail = true;
		
		if ( !errorMail && !errorCampos  ) {
		
		        $.get("/system/modules/com.tfsla.diario.elcronista/elements/CC_Common_SendMail.jsp",
		        	{ nombre:_nombre, apellido:_apellido, email:_email, telefono:_telefono, dirigido:_dirigido, comentario:_comentario }, function(data){
		          	$(".form-popup").html(data);
		        });
		        
		} else {
			if ( errorCampos ) alert("Complete todos los campos."); 
			if ( errorMail ) alert("Ingrese una dirección de correo válida.");
        	}
	});
});

function isValidEmailAddress( emailAddress ) {
	var pattern = new RegExp(/^((([a-z]|\d|[!#$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
	return pattern.test( emailAddress );
};


</script>


