	function recargaTagsCarrucel(elemento){   
		$.post("/system/modules/com.tfsla.diario.elcronista/elements/CC_Common_TagsCarrusel.jsp",{ palabraClave: elemento}, function(data){
			$("#tagsRecargados").html(data);
			$('#foo').carouFredSel({
				width: 890,
				height: 'auto',
				prev: '#prev',
				next: '#next',
				auto: false
			});
		});
	}
</script>


