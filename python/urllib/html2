  	<SCRIPT LANGUAGE="JavaScript">
  		$(document).ready(function() {
			getPopUps();
		});
  	
		function getPopUps(){
			$.get("http://www.cronista.com/usuarios/login_header.html", 
				{R:'/index.html'}, function(data){
				$('#login').html(data);
			});
			$.get("http://www.cronista.com/usuarios/registration_header.html", function(data){
				$('#register').html(data);
			});
		}
	</script>
  	      	
