<script>
if(typeof String.prototype.trim !== 'function') {
  String.prototype.trim = function() {
    return this.replace(/^\s+|\s+$/g, ''); 
  }
}
$(document).ready(function(){
// Quitar la funcionalidad de encuesta.
var form_encuesta = $("#encuesta");
var btn_submit = $("#encuesta .btns_encuesta a.btn_votar");
var on_click = btn_submit.attr("onclick");
btn_submit.onclick = undefined;
btn_submit.removeAttr('onclick');
/*
$(".bcaptcha")
    .appendTo("#encuesta");*/
$(".bcaptcha").hide();
$(".enviar_voto").click(function(e){
	e.preventDefault();
	$("#captchaForm").submit();
	
});
$("#encuesta .btns_encuesta a.btn_votar").click(function(e){
	e.stopImmediatePropagation();    		
	e.preventDefault();
	e.stopPropagation();
	var rta = $('input:radio[name=respuesta]:checked').val();
	if( rta == undefined || rta == null){
		alert("Por favor, seleccione una respuesta");
		return false;
	}
	//$.get( "/system/modules/com.tfsla.diario.elcronista/elements/CC_Common_BrandCaptcha_Async.jsp", function( data ) {
	//  $( "#captchaContainer" ).append( data );
	  	 BrandCaptcha.initAsync("22d06b1623386ad8bf7512942ec8d9c5481dc3ef", "captchaContainer");	
	  	$("#encuesta").hide();
		$(".bcaptcha").show();
	//});	
	
	return false;
});
$( "#captchaForm" ).submit(function( event ) {
 
  // Stop form from submitting normally
  event.preventDefault();
 	
	$.post( $("#captchaForm").attr("action"), $( "#captchaForm" ).serialize(), function( data ) {
	//console.log(data);
	  if(data.trim() == "ok"){
	  	sbtPoll();
	  }else{
	 	BrandCaptcha.reload()
	  	alert("Por favor vuelva a ingresar el captcha");
	  }
	} );
});

	
	function sbtPoll(){
		
		//console.log(":: :  :  : Encusta : : : : : : ::");
		
		//btn_submit[on_click ](); 
		var regExp = /\(([^)]+)\)/;
		var matches = regExp.exec(on_click);
		//console.log(matches[1]);
		var find = '\'';
		var re = new RegExp(find, 'g');
		
		var  params = matches[1].replace(re, '');
		
		params = params.split(",");
		
		submitPool(params[0], params[1]);
		$(".bcaptcha").hide();
		$("#encuesta").show();
		
		
		/*var fn = window["submitPool"];
		fn(Array.prototype.slice(matches[1]));
		$(".bcaptcha").hide();
		$("#encuesta").show();
		*/

	}
	
});
</script> 
		
			
					
	
  	

